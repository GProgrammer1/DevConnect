{
  "expo": {
    "name": "DevConnect",
    "slug": "DevConnect",
    "splash": {
      "backgroundColor": "#0b0f14"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "devconnect",
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#0b0f14",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "backgroundColor": "#0b0f14",
      "navigationBar": {
        "backgroundColor": "#0b0f14",
        "barStyle": "light-content"
      },
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "src/assets/images/favicon.png",
      "bundler": "metro",
      "backgroundColor": "#0b0f14",
      "themeColor": "#0b0f14"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "src/assets/images/splash-icon.png"
        }
      ],
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "3eb732be-32e9-4c20-8124-902bbd178313"
      }
    }
  }
}
